{
  "rules": {
    "lists": {
      "$list": {
        ".read": "data.child('users').hasChild(auth.id)",
        ".write": "data.child('users').hasChild(auth.id)"
      }
    },
    "users": {
      "$user": {
        ".read": "$user == auth.id",
        ".write": "$user == auth.id"
      }
    }
  }
}
